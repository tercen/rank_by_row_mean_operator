{
  "name": "Rank by Row Means",
  "description": "Ranks the incoming data by row means.",
  "tags": [
    "preprocessing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/rank_by_row_mean_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Row factor(s) to compute means and ranks for",
            "ontologyMapping": "row",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Numeric values for y-axis",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}